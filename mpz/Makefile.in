INCLUDES = -I. -I.. -I../mpn -I$(srcdir)/.. -I$(srcdir)

libmpz.a: Makefile $(MPZ_OBJS)
	rm -f $@
	$(AR) $(AR_FLAGS) $@ $(MPZS_OBJS) $(MPZ_OBJS)

.c.o:
	@echo "Compiling $<"
	@$(CC) -c -DGMP_SMALL $(INCLUDES) $(CFLAGS) $(XCFLAGS) $< -o zs`basename $@`
	@$(CC) -c -UGMP_SMALL $(INCLUDES) $(CFLAGS) $(XCFLAGS) $<

clean mostlyclean:
	rm -f *.o zs*.c libmpz.a
	@cd tests; $(MAKE) $@
distclean maintainer-clean: clean
	rm -f Makefile config.status
	@cd tests; $(MAKE) $@

Makefile: $(srcdir)/Makefile.in
	$(SHELL) ./config.status

